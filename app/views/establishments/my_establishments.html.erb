        <td><%= followed.end_date.present? ? followed.end_date.strftime('%d/%m/%Y') : '-' %></td>
        <td><%= followed.status %></td>
        <td><%= link_to 'Voir', establishment_path(followed.establishment) %></td>
      </tr>
    <% end %>
