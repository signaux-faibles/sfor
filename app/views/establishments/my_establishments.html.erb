    </thead>
    <tbody>
    <% @establishments_followed.each do |followed| %>
      <tr>
        <td><%= followed.establishment.siret %></td>
        <td><%= followed.establishment.siren %></td>
        <td><%= followed.establishment.raison_sociale %></td>
